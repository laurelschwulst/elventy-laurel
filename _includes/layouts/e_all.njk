<div class="grid">

  {% for e in e_all %}
    <div class="grid-item">
      <a href="{{ e.url }}">
        <figure class="image">
            <img src="{{ e.data.featured_image }}">
        </figure>
      </a>
      <a href="{{ e.url }}">
        <div class="title">
            {{ e.data.title }}
            <div class="gray">{{ e.data.subtitle }}</div>
        </div>
      </a>
    </div>
  {% endfor %}

</div>